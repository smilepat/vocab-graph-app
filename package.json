{
    "name": "ontology-vocab-app",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev": "concurrently \"npm run dev -w @vocab/web\" \"npm run dev -w api\"",
        "build": "npm run build --workspaces --if-present"
    },
    "devDependencies": {
        "concurrently": "^9.2.1"
    }
}